generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Job {
  id         Int      @id @default(autoincrement())
  title      String
  location   String
  salary     String?
  datePost   DateTime @default(now())
  experience Int?
  jobType    String?
}
